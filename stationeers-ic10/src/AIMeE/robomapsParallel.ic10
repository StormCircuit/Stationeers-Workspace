move sp 16
move r0 LastWaypoint #mult lastWaypoint as we compare it directly to SP
mul r0 r0 2
s db Setting r0 #this gets loaded by roboroutine, is how many waypoints
aimeeStackLoadLoop:
beq sp 0 aimeeStackLoadLoopEnd
pop r0
lbn r0 AIMEEhousing r0 ReferenceId 1
beqz r0 aimeeStackLoadLoop
put r0 0 x1
put r0 1 z1
put r0 2 x2
put r0 3 z2
put r0 4 x3
put r0 5 z3
put r0 6 x4
put r0 7 z4
put r0 8 x5
put r0 9 z5
put r0 10 x6
put r0 11 z6
put r0 12 x7
put r0 13 z7
put r0 14 x8
put r0 15 z8
#resets mining site error status on AIMEE unit
s r0 Setting 0
j aimeeStackLoadLoop
aimeeStackLoadLoopEnd:
#program end