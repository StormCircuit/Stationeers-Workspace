#Instructions:
#1. Using GPS tablet and write down X and Z coordinates (ignore y)
#2. Put them in for waypoints below.
#2a. If you do not need 8 waypoints, set LastWaypoint to the number you need (max 8)
#3. Rename up to 6 COMPACT IC10 housings: "aimee1", "aimee2", ... , "aimee6"
#4. To update waypoints, edit coordinates and LastWaypoint and reinsert THIS ic10 chip
define LastWaypoint 5
define x1 -918
define z1 -1290

define x2 -912
define z2 -1286

define x3 -913
define z3 -1282

define x4 -924
define z4 -1282

define x5 -1028
define z5 -1243

define x6 0
define z6 0

define x7 0
define z7 0

define x8 0
define z8 0
#Code below
define AIMEEhousing HASH("StructureCircuitHousingCompact")
define AIMEE1 HASH("aimee1")
define AIMEE2 HASH("aimee2")
define AIMEE3 HASH("aimee3")
define AIMEE4 HASH("aimee4")
define AIMEE5 HASH("aimee5")
define AIMEE6 HASH("aimee6")
define AIMEE7 HASH("aimee7")
define AIMEE8 HASH("aimee8")
define AIMEE9 HASH("aimee9")
define AIMEE10 HASH("aimee10")
define AIMEE11 HASH("aimee11")
define AIMEE12 HASH("aimee12")
define AIMEE13 HASH("aimee13")
define AIMEE14 HASH("aimee14")
define AIMEE15 HASH("aimee15")
define AIMEE16 HASH("aimee16")
put db 0 AIMEE1
put db 1 AIMEE2
put db 2 AIMEE3
put db 3 AIMEE4
put db 4 AIMEE5
put db 5 AIMEE6
put db 6 AIMEE7
put db 7 AIMEE8
put db 8 AIMEE9
put db 9 AIMEE10
put db 10 AIMEE11
put db 11 AIMEE12
put db 12 AIMEE13
put db 13 AIMEE14
put db 14 AIMEE15
